<mapper namespace="com.myproject.parking.lib.mapper.LookupMapper">
    
    <select id="findLookupAll" resultType="Lookup">
        select l.id as id,
        	l.value as value,
        	l.description as description,
        	l.product_id as productId
        from smis_lookup l
        left join smis_product p on p.id=l.product_id
    </select>

</mapper>
